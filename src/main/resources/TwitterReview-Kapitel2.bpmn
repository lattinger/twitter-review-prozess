      <bpmn:outgoing>SequenceFlow_1nms2zv</bpmn:outgoing>
      <bpmn:script>emailAdress = execution.getVariable("email");
var alias = emailAdress.substring(0,4);
execution.setVariable("alias",alias);</bpmn:script>
    </bpmn:scriptTask>
