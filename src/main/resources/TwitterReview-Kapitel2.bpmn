            <camunda:value id="accepted" name="Genehmigt" />
            <camunda:value id="revisable" name="Zu überarbeiten" />
            <camunda:value id="rejected" name="Abgelehnt" />
          </camunda:formField>
          <camunda:formField id="checkResultComment" label="Hinweise für Mitarbeiter zum Prüfergebnis" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_0glyf4v</bpmn:incoming>
